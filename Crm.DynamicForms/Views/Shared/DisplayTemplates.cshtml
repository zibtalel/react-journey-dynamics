@inherits Crm.Library.Modularization.CrmViewUserControl<Crm.ViewModels.CrmModelList<string>>
@using Crm.Library.Extensions

@(Html.HiddenTranslations(Model.List.ToArray()))
@foreach (var elementType in Model.List) { 
	<script type="text/html" id="dynamicform-element-display-template-@(elementType)">
	<div data-bind="css: CssExtra(), attr:{id:'formelement-' + Id()}, visible: $data.isVisible">
 			 @await Html.PartialAsync(String.Format("DisplayTemplates/{0}", elementType)) 
		</div>
	</script>
 } 

