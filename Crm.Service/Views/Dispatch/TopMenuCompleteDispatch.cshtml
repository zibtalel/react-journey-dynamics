@inherits Crm.Library.Modularization.CrmViewUserControl
@using Crm.Library.Globalization.Extensions

<li data-bind="visible: $root.dispatch().StatusKey() === 'InProgress' || $root.dispatch().StatusKey() === 'SignedByCustomer', tooltip: true" data-placement="bottom" title="@(Html.Localize("Complete"))">
	<a id="change-status" href="#" data-bind="attr: { 'data-route' : 'Crm.Service/Dispatch/ChangeStatusTemplate/' + $root.dispatch().Id() }" data-toggle="modal" data-target="#modal">
		<i class="tm-icon zmdi zmdi-mail-send"></i>
	</a>
</li>

