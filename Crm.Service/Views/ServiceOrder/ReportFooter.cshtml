@inherits Crm.Library.Modularization.CrmViewPage
@{
    Layout = "~/Views/Shared/Layout/HtmlTemplateLayout.cshtml";
}
@using Crm.Library.Globalization.Extensions

@section TemplateMainContent {

    <div class="reportcontainer">
        <div class="report">
            <div style="padding: 10px 0 10px 0; min-height: 60px; position: relative; display: flex; flex-direction: column; justify-content: space-between;">
                <div style="flex-grow: 1;">
                    @await Html.PartialAsync("Report/FooterContent")
                </div>
                <div style="position: absolute; bottom: 0; right: 0; white-space: nowrap; font-size: 10px !important; margin-bottom: 5px;">
                    @(Html.Localize("Page")) <span>[page]</span> @(Html.Localize("of")) <span>[toPage]</span>
                </div>
            </div>
        </div>
    </div>

}
